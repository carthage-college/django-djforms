{% autoescape off %}{% load humanize %}
{% spaceless %}
{
    "count": "{{count|intcomma}}",
    "percent": "{{percent}}",
    "total": "{{promo.total|intcomma}}",
    "title": "{{promo.title}}",
    "results": [
    {% for obj in donors %}
        {% ifchanged obj.last_name and ifchanged obj.first_name %}
        {
            "last_name": "{{obj.last_name|title|escapejs}}",
            "first_name": "{{obj.first_name|title|escapejs}}",
            "class_of": "{{obj.class_of}}",
            "relation": "({{obj.relation}})"
        }
        {% if not forloop.last %},{% endif %}
        {% endifchanged %}
    {% endfor %}
    ]
}
{% endspaceless %}
{% endautoescape %}
